{
  "name": "megan-ai",
  "displayName": "Megan AI",
  "version": "0.0.1",
  "description": "",
  "main": "./core/vscode-integration.js",
  "engines": {
    "vscode": "^1.89.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "contributes": {
    "commands": [
      {
        "command": "meganai.activate",
        "title": "Start Megan AI"
      },
      {
        "command": "meganai.explainCode",
        "title": "Explain with Megan AI"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "meganai.explainCode",
          "group": "navigation",
          "when": "editorHasSelection"
        }
      ]
    }
  },
  "scripts": {
    "compile": "esbuild ./core/vscode-integration.js --bundle --outfile=out/extension.js --platform=node",
    "watch": "npm run compile -- --watch",
    "package": "vsce package",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@vscode/vsce": "^3.6.0",
    "esbuild": "^0.25.8",
    "ollama": "^0.5.16",
    "rimraf": "^6.0.1",
    "vscode-languageclient": "^9.0.1"
  },
  "devDependencies": {},
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs"
}